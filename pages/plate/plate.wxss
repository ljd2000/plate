/* pages/plate/plate.wxss */
image{
    width: 100%;
    height: 50rpx;
    border: 0px solid red;
}
.btn1 {
    width: 80%;
    margin-top: 20rpx;
    background-color: burlywood;
    color: white;
    border-radius: 20rpx;
    display: flex;
    flex-direction: row;
    align-items: center;
    justify-content: center;
  }

  .btn2 {
    width: 80%;
    margin-top: 10rpx;
    background-color: green;
    color: white;
    border-radius: 20rpx;
    display: flex;
    flex-direction: row;
    align-items: center;
    justify-content: center;
  }
  .btnImg {
    margin-left:20rpx;
    width: 46rpx;
    height: 46rpx;
  }

  .btn2.button-hover {
    background-color: grey;
  }

  .result {
    align-items: center;
  }

  .box{
    display: flex;
  }

  .box1{
    height:80rpx;
    width: 80rpx;
    margin-top: 10rpx;
    align-items: center;
    text-align:center;
    vertical-align: middle;
    justify-content: center;
    margin-bottom: 10rpx;
    border: 1px solid #d2d2d2;
    border: 1px solid burlywood;
    background-color: burlywood;
    border-radius: 20rpx;
  }

  .input-text {
    height:80rpx;
    margin-top: 10rpx;
    margin-bottom: 60rpx;
    border: 1px solid #d2d2d2;
    background: url(data:image/png;base64,) no-repeat 20rpx;
    border-radius: 20rpx;
    padding-left: 100rpx;

    outline: none;
  }
